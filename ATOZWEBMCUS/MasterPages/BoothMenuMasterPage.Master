            </asp:Menu>
        </div>

        <div>
            <asp:LinkButton ID="linkSMSReload" runat="server" Font-Bold="True" Font-Size="Large" ForeColor="Red" Height="70px" OnClick="linkSMSReload_Click" >LinkButton</asp:LinkButton>
        </div>
        <div id="DivSMS" runat="server" align="center">
            <br />
